{
  "name": "sitemill/craft-powertools",
  "description": "A set of tools used internally by Sitemill",
  "type": "craft-plugin",
  "version": "1.0.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "powertools"
  ],
  "support": {
    "docs": "https://github.com/sitemill/craft-powertools/blob/master/README.md",
    "issues": "https://github.com/sitemill/craft-powertools/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Sitemill",
      "homepage": "https://sitemill.co"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^7.0.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "craftcms/cms": "^3.2.0",
    "vlucas/phpdotenv": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "sitemill\\powertools\\": "src/"
    }
  },
  "extra": {
    "name": "Powertools",
    "handle": "powertools",
    "developer": "sitemill",
    "developerUrl": "https://sitemill.co",
    "documentationUrl": "https://github.com/sitemill/craft-powertools/blob/master/README.md",
    "changelogUrl": "https://raw.githubusercontent.com/sitemill/craft-powertools/master/CHANGELOG.md",
    "components": {
      "powertoolsService": "PhoneHomeService"
    },
    "class": "sitemill\\powertools\\Powertools"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
